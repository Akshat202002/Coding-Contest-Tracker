{
    "short_name": "Coding Contest Tracker",
    "name": "Coding Contest Tracker",
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
    "icons": {
      "16": "favicon.ico",
      "48": "logo192.png",
     "128": "logo512.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "https://coding-contest-tracker-default-rtdb.firebaseio.com/",
        "https://apis.google.com/"
    ],
    "manifest_version": 2,
    "version": "0.0.1",
    "browser_action": {
      "default_popup": "index.html",
      "default_title": "Coding Contest Tracker"
    }
}